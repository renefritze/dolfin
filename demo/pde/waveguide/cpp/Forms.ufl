element = FiniteElement("Nedelec 1st kind H(curl)", "triangle", 2)

v = TestFunction(element)
u = TrialFunction(element)

def curl_t(w):
    return Dx(w[1], 0) - Dx(w[0], 1)

a = curl_t(v)*curl_t(u)*dx
L = inner(v, u)*dx
