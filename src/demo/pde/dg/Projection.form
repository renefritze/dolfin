disc_elem =  FiniteElement("Discontinuous Lagrange", "triangle", 1)
const_elem = FiniteElement("Lagrange", "triangle", 1)

v = TestFunction(const_elem)
u = TrialFunction(const_elem)

f = Function(disc_elem)

a = v*u*dx

L = v*f*dx
