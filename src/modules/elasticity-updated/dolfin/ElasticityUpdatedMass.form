name = "ElasticityUpdatedMass"
element = FiniteElement("Vector Lagrange", "tetrahedron", 1)
element2 = FiniteElement("Lagrange", "tetrahedron", 1)

u = BasisFunction(element)
v = BasisFunction(element)
rho = Function(element2) # Density

i = Index()

a = rho * u[i] * v[i] * dx
