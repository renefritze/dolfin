# Test form for updated elasticity

name = "ElasticityUpdated"
element1 = FiniteElement("Discontinuous vector Lagrange", "tetrahedron", 0)
element2 = FiniteElement("Vector Lagrange", "tetrahedron", 1)

nu = Constant()	# viscosity coefficient

v = BasisFunction(element2)
f = Function(element2)
sigma0 = Function(element1)
sigma1 = Function(element1)
sigma2 = Function(element1)
sigmav0 = Function(element1)
sigmav1 = Function(element1)
sigmav2 = Function(element1)

L = (f[i] * v[i] -
	(sigma0[i] * v[0].dx(i) +
	sigma1[i] * v[1].dx(i) +
	sigma2[i] * v[2].dx(i)) -
	nu * (
	sigmav0[i] * v[0].dx(i) +
	sigmav1[i] * v[1].dx(i) +
	sigmav2[i] * v[2].dx(i))) * dx

