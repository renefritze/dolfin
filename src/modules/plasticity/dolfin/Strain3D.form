elementA = FiniteElement("Discontinuous vector Lagrange", "tetrahedron", 0, 6)
elementU = FiniteElement("Vector Lagrange", "tetrahedron", 1, 3)

v = TestFunction(elementA)
w = TrialFunction(elementA)

u = Function(elementU)

def eps(u):	#eps_xx			eps_yy		 eps_zz		    		gam_xy			 					gam_xz								gam_yz
	return [u[0].dx(0), u[1].dx(1), u[2].dx(2), u[0].dx(1) + u[1].dx(0), u[0].dx(2) + u[2].dx(0), u[1].dx(2) + u[2].dx(1)]

a = dot(v, w)*dx
L = dot(v, eps(u))*dx
