elementA = FiniteElement("Discontinuous vector Lagrange", "triangle", 0, 3)
elementU = FiniteElement("Vector Lagrange", "triangle", 1)

v = TestFunction(elementA)
w = TrialFunction(elementA)

u = Function(elementU)


def eps(u): #eps_xx   eps_yy    (gamma_xy)
	return [u[0].dx(0), u[1].dx(1), u[0].dx(1) + u[1].dx(0)]

a = dot(v, w)*dx
L = dot(v, eps(u))*dx
