elementA = FiniteElement("Vector Lagrange", "tetrahedron", 1, 1)
elementB = FiniteElement("Discontinuous vector Lagrange", "tetrahedron", 0, 1)

v = TestFunction(elementA)
w = TrialFunction(elementA)

u = Function(elementB)

a = dot(v, w)*dx
L = dot(v, u)*dx
